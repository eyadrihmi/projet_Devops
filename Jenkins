pipeline {
    agent any
    
    environment {   
        dockerImage = ''
        registry = "eyadrihmi/achat"
    }
    
    stages {
        stage('GIT') { 
            steps {
                git url:'https://github.com/eyadrihmi/projet_Devops.git'
            }
        }
        
      stages {
        stage('Sending docker file to ansible server ') { 
            sshagent(['ansible']) {
            sh 'ssh -o StrictHostKeyChecking=no centos@172.29.12.70'
            sh 'scp /var/lib/jenkins/workspace/back_devops/* centos@172.29.12.70:/root/Desktop'
           }
        }
    
        
    }
}
